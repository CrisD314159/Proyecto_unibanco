<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="363.0" prefWidth="591.0" stylesheets="@../Stylesheets/BackgroundFont.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PanelPrincipalController">
   <children>
      <Label fx:id="welcomeLabel" contentDisplay="RIGHT" layoutX="161.0" layoutY="111.0" prefHeight="44.0" prefWidth="269.0" text="Bienvenido a Unibanco" textAlignment="JUSTIFY" textOverrun="CLIP">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <Button fx:id="depositoButton" layoutX="104.0" layoutY="226.0" mnemonicParsing="false" onAction="#realizarDepositoAction" prefHeight="44.0" prefWidth="122.0" text="Realizar deposito" />
      <Button fx:id="retiroButton" layoutX="245.0" layoutY="226.0" mnemonicParsing="false" onAction="#realizarRetiroAction" prefHeight="44.0" prefWidth="102.0" text="Realizar retiro" />
      <Button fx:id="saldoButton" layoutX="373.0" layoutY="226.0" mnemonicParsing="false" onAction="#consultarSaldoAction" prefHeight="44.0" prefWidth="114.0" text="Consultar saldo" />
      <Label layoutX="149.0" layoutY="310.0" text="No tienes cuenta en Unibanco?" textFill="#7c7878" />
      <Button fx:id="crearCuentaButton" layoutX="341.0" layoutY="306.0" mnemonicParsing="false" onAction="#crearCuentaAction" prefHeight="28.0" prefWidth="122.0" text="Crea una aqui!!" textFill="#949494" />
   </children>
</AnchorPane>
